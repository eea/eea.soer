<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>

<metal:main metal:fill-slot="main">
<tal:main-macro metal:define-macro="main"
           tal:define="text python: here.CookedBody(stx_level=2)">

        <div metal:use-macro="here/document_actions/macros/document_actions">
            Document actions (print, sendto etc)
        </div>

        <h1 tal:content="object_title" class="documentFirstHeading">
          Title or id
        </h1>
    
        <div metal:use-macro="here/document_byline/macros/byline">
          Get the byline - contains details about author and modification date.
        </div>


<metal:isFromFeed define-macro="isFromFeed">
  <tal:autoUpdated tal:condition="context/isFromFeed|nothing">
	  <div class="header">
            <dl class="portlet portlet-rss">
	      <dt class="portletHeader">
		Related topics
	      </dt>
	      <dt class="portletItem">
		<a href="http://www.eea.europa.eu/themes/biodiversity">
		  <img src="http://www.eea.europa.eu/themes/biodiversity/theme_image/image_icon" alt="Biodiversity"> Biodiversity
		</a>
	      </dt>
	      
	      <dt class="portletItem">
		<a href="http://www.eea.europa.eu/themes/policy">
		  <img src="http://www.eea.europa.eu/themes/air/theme_image/image_icon" alt="Air polution"> Air polution
		</a>
	      </dt>
	      
	      <dd class="portletFooter">
	      </dd>
	    </dl>


	    <dl id="channel-portlet" class="portlet" >
	      <dt class="portletHeader">	
		<a href="" title="" class="websource-box fancybox"
		   tal:attributes="href string:#channelSource;
				   title string:Channel source">Channel</a>
               <div class="hiddenStructure">
                   <div id="channelSource" class="websource-info" >
                      <img src="miljostatus-logo-eng.gif" />
                      <br clear="all" />
                      <dl>
                         <dt>Organisation name</dt>
                         <dd>Organisation name</dd>
                         <dt>Reportin country</dt>
                         <dd>Norway</dd>
                         <dt>Organisation website</dt>
                         <dd><a href="" title="">Organisation website</a></dd>
                         <dt>Contact link</dt>
                         <dd><a href="" title="">Contact link</a></dd>
                         <dt>Last updated</dt>
                         <dd>2010-05-07</dd>
                         <dt>Content license</dt>
                         <dd><a href="" title="">CC By 2.5</a></dd>
		      </dl>
                    </div>
                </div>
		Content provider</dt>
	      <dd class="portletItem">
		<img src="miljostatus-logo-eng.gif" /></dd>
	    </dl>
	    <dl id="location-portlet" class="portlet" >
	      <dt class="portletHeader">	
		<a href="" title="" class="websource-box fancybox"
		   tal:attributes="href string:#locationSource;
				   title string:Location">Source</a>
               <div class="hiddenStructure">
                   <div id="locationSource" class="websource-info">
                      <dl>
                         <dt>Geocoverage</dt>
                         <dd>Norway</dd>
		      </dl>
                  </div>
                </div>
		Geocoverage</dt>
	      <dd class="portletItem">
		<img src="200px-Europe-Norway.svg.png" title="Norway" /></dd>
	    </dl>

	  </div>
  </tal:autoUpdated>
</metal:isFromFeed>

        <p class="documentDescription"
           tal:content="here/Description"
           tal:condition="here/Description">
            Description
        </p>
        <p tal:condition="python: not text and is_editable"
           i18n:translate="no_body_text"
           class="discreet">
            This item does not have any body text, click the edit tab to change it.
        </p>
    
        <div class="stx"
             tal:condition="text"
             tal:attributes="class python:test(here.Format() in ('text/structured',
                                                   'text/x-rst', ), 'stx', 'plain')">
            <div tal:replace="structure text" />
        </div>

<p tal:replace="structure context/getKeyMessage|nothing" />

<ul>
    <li tal:content="here/getLongSoerTopic"></li>
    <li tal:content="here/getLongSoerQuestion"></li>
    <li tal:content="here/getSoerCountryName"></li>
</ul>

