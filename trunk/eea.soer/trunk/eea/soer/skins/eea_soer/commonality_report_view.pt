<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>

<metal:main metal:fill-slot="main">
<tal:main-macro metal:define-macro="main">

<metal:isFromFeed define-macro="isFromFeed">
  <tal:autoUpdated tal:condition="context/isFromFeed|nothing">
          <div class="portalMessage"
               i18n:translate="automatic_updated_report_info" 
	       tal:condition="not: isAnon">
                This report is updated automatically from remote Feed. Please do not make any manual changes, they will be lost during next update.
            </div>
          <div class="portalMessage"
               i18n:translate="automatic_updated_report_info" 
	       tal:condition="isAnon">
                This report is updated automatically from <a href="" title="" 
		                                             tal:attributes="href context/original_url;
							                     title context/Title">remote source</a>.
            </div>
  </tal:autoUpdated>
</metal:isFromFeed>
<metal:document use-macro="here/document_macros/macros/document">
    <tal:document metal:fill-slot="related_multimedia">
        <div id="related-portlets">
        <metal:themes use-macro="here/portlet_themes_object/macros/portlet" />
        <div metal:use-macro="here/document_relateditems/macros/multimedia">
            show related multimedia items if they exist
        </div>
        <div metal:use-macro="here/document_relateditems/macros/pages">
            show related pages if they exist
        </div>
        <div metal:use-macro="here/document_relateditems/macros/auto">
            show auto related items if they exist
        </div>
        </div>
    </tal:document>
</metal:document>
<p tal:replace="structure context/getKeyMessage|nothing" />

<ul>
    <li tal:content="here/getLongSoerTopic"></li>
    <li tal:content="here/getLongSoerQuestion"></li>
    <li tal:content="here/getSoerCountryName"></li>
</ul>

<div metal:define-macro="figures-album" class="map-photo-album">
  <h4 id="fancy-edse">Figures</h4>
  <metal:listing use-macro="here/atct_album_view/macros/listing" />
</div>

</tal:main-macro>
</metal:main>

</body>
</html>

